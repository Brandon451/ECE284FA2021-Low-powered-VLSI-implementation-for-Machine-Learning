    %load/vec4a v0x1c35b40, 4;
    %load/vec4 v0x1c35be0_0;
    %cmp/e;
    %jmp/0xz  T_2.4, 4;
    %vpi_call 2 80 "$display", "%2d-th read data is %h --- Data matched", v0x1c36030_0, v0x1c35be0_0 {0 0 0};
