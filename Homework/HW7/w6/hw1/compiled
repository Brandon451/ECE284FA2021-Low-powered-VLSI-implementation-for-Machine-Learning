    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1c35cb0_0, 0, 1;
