 .timescale -9 -12;
P_0x1c13630 .param/l "col" 0 2 26, +C4<00000000000000000000000000000000>;
P_0x1c13670 .param/l "run_cycle" 0 2 25, +C4<00000000000000000000000001000000>;
v0x1c35750_0 .var "A", 10 0;
