    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %load/vec4 v0x1c35750_0;
    %addi 1, 0, 11;
    %store/vec4 v0x1c35750_0, 0, 11;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x1c35f50_0, 0, 32;
