v0x1c350c0_0 .net "WEN", 0 0, v0x1c35d50_0;  1 drivers
v0x1c35180_0 .net *"_s0", 31 0, L_0x1c362d0;  1 drivers
v0x1c35260_0 .net *"_s2", 12 0, L_0x1c363f0;  1 drivers
L_0x7ffa09607018 .functor BUFT 1, C4<00>, C4<0>, C4<0>, C4<0>;
v0x1c35340_0 .net *"_s5", 1 0, L_0x7ffa09607018;  1 drivers
v0x1c354b0_0 .var "add_q", 10 0;
v0x1c35590 .array "memory", 0 2047, 31 0;
E_0x1c11850 .event posedge, v0x1c34de0_0;
L_0x1c362d0 .array/port v0x1c35590, L_0x1c363f0;
L_0x1c363f0 .concat [ 11 2 0 0], v0x1c354b0_0, L_0x7ffa09607018;
    .scope S_0x1c1aac0;
T_0 ;
    %wait E_0x1c11850;
    %load/vec4 v0x1c34d20_0;
    %nor/r;
    %load/vec4 v0x1c350c0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x1c1ad30_0;
    %assign/vec4 v0x1c354b0_0, 0;
T_0.0 ;
    %load/vec4 v0x1c34d20_0;
    %nor/r;
    %load/vec4 v0x1c350c0_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x1c34eb0_0;
    %load/vec4 v0x1c1ad30_0;
    %pad/u 13;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x1c35590, 0, 4;
T_0.2 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1c00ae0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
