    .scope S_0x1c00ae0;
T_2 ;
    %vpi_call 2 39 "$dumpfile", "sram_tb.vcd" {0 0 0};
    %vpi_call 2 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1c00ae0 {0 0 0};
    %vpi_func 2 42 "$fopen" 32, "activation.txt", "r" {0 0 0};
    %store/vec4 v0x1c36110_0, 0, 32;
    %vpi_func 2 45 "$fscanf" 32, v0x1c36110_0, "%s", v0x1c35eb0_0 {0 0 0};
    %store/vec4 v0x1c361f0_0, 0, 32;
    %vpi_func 2 46 "$fscanf" 32, v0x1c36110_0, "%s", v0x1c35eb0_0 {0 0 0};
    %store/vec4 v0x1c361f0_0, 0, 32;
    %vpi_func 2 47 "$fscanf" 32, v0x1c36110_0, "%s", v0x1c35eb0_0 {0 0 0};
