    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x1c36030_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x1c36030_0;
    %cmpi/s 63, 0, 32;
    %jmp/0xz T_2.1, 5;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %load/vec4 v0x1c35750_0;
    %addi 1, 0, 11;
    %store/vec4 v0x1c35750_0, 0, 11;
    %vpi_func 2 61 "$fscanf" 32, v0x1c36110_0, "%32b", v0x1c35aa0_0 {0 0 0};
    %store/vec4 v0x1c361f0_0, 0, 32;
    %load/vec4 v0x1c35aa0_0;
    %ix/getv/s 4, v0x1c36030_0;
    %store/vec4a v0x1c35b40, 4, 0;
    %vpi_call 2 63 "$display", "%2d-th written data is %h", v0x1c36030_0, v0x1c35aa0_0 {0 0 0};
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %load/vec4 v0x1c36030_0;
    %addi 1, 0, 32;
    %store/vec4 v0x1c36030_0, 0, 32;
