    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1c359d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x1c36030_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x1c36030_0;
    %cmpi/s 63, 0, 32;
    %jmp/0xz T_2.3, 5;
    %delay 20000, 0;
